<?xml version="1.0" encoding="utf-8"?>
<package>
    <metadata>
        <id>MyCouch</id>    
        <title>MyCouch - CouchDb client</title>
        <version>$version$</version>
        <authors>danielwertheim</authors>
        <copyright>Copyright 2013 Daniel Wertheim</copyright>
        <summary>Simple async CouchDb client.</summary>
        <description>
            A Simple CouchDb client that uses the asynchronous HttpClient. Lets you work with native JSON documents as well as entities. MyCouch does NOT require you to implement any specific interfaces or base-classes. MyCouch is pluggable. If you do not like something....switch it out. MyCouch comes with simple model conventions as automatically injecting $doctype.
        </description>
        <projectUrl>https://github.com/danielwertheim/MyCouch</projectUrl>
        <licenseUrl>https://github.com/danielwertheim/MyCouch#license</licenseUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <releaseNotes>Can be found on project site: https://github.com/danielwertheim/mycouch/wiki/release-notes</releaseNotes>        
        <tags>couchdb nosql json document-db db poco async, asynchronous</tags>
        <frameworkAssemblies>
            <frameworkAssembly assemblyName="System.Net.Http" targetFramework="net40" />
        </frameworkAssemblies>
        <dependencies>
            <dependency id="Ensure.That" version="0.10.1" />
            <dependency id="Newtonsoft.Json" version="5.0.4" />
        </dependencies>
    </metadata>
    <files>
      <file src="*\MyCouch.dll" target="lib\net40" />
      <file src="*\MyCouch.xml" target="lib\net40" />
    </files>
</package>